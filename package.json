{
  "name": "react-clock",
  "version": "3.0.0",
  "description": "An analog clock for your React app.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:production",
    "build-old": "yarn build-js-all && yarn copy-styles && yarn build-styles",
    "build-js-all": "yarn build-js-esm && yarn build-js-umd",
    "build-js-esm": "BABEL_ENV=production-esm babel src -d dist/esm --ignore \"**/*.spec.js,**/*.spec.jsx\"",
    "build-js-umd": "BABEL_ENV=production-umd babel src -d dist/umd --ignore \"**/*.spec.js,**/*.spec.jsx\"",
    "build-styles": "lessc ./dist/Clock.less ./dist/Clock.css",
    "clean": "rimraf dist",
    "copy-styles": "node ./copy-styles.js",
    "jest": "jest",
    "jest-coverage": "jest --coverage",
    "lint": "eslint sample/ src/ test/ --ext .jsx,.js",
    "prepack": "yarn clean && yarn build",
    "test": "yarn lint && yarn jest"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "collectCoverageFrom": [
      "**/src/**.{js,jsx}",
      "!**/src/index.js"
    ]
  },
  "keywords": [
    "clock",
    "digital clock",
    "analog clock",
    "time",
    "react"
  ],
  "author": {
    "name": "Wojciech Maj",
    "email": "kontakt@wojtekmaj.pl"
  },
  "contributors": [
    {
      "name": "Yin Hengli",
      "email": "yinhengliben@gmail.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@wojtekmaj/date-utils": "^1.0.0",
    "clsx": "^1.1.1",
    "react-jss": "^10.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.1",
    "babel-eslint": "^10.0.0",
    "enzyme": "^3.10.0",
    "eslint": "^7.12.0",
    "eslint-config-wojtekmaj": "^0.5.0",
    "jest": "^26.6.0",
    "less": "^3.8.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rollup": "^2.39.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "rimraf": "^3.0.0",
    "tsdx": "^0.14.1",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "react": "^15.5.0 || ^16.0.0 || ^17.0.0-0",
    "react-dom": "^15.5.0 || ^16.0.0 || ^17.0.0-0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/",
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wojtekmaj/react-clock.git"
  },
  "funding": "https://github.com/wojtekmaj/react-clock?sponsor=1"
}
